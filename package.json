{
  "name": "purranormal-activity",
  "version": "0.14.8",
  "private": true,
  "scripts": {
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "dev": "npm run increase && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "format": "biome format . --write",
    "lint:fix": "biome check --write .",
    "generate": "pnpm run increase && npx drizzle-kit push",
    "increase": "npm --no-git-tag-version version patch",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome format --write"
    ]
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.92",
    "@aws-sdk/client-s3": "3.995.0",
    "@libsql/client": "^0.17.0",
    "@tabler/icons-react": "^3.37.1",
    "ai": "^5.0.137",
    "classnames": "^2.5.1",
    "dotenv": "^17.3.1",
    "drizzle-orm": "^0.45.1",
    "next": "15.5.6",
    "next-view-transitions": "^0.3.5",
    "nuqs": "^2.8.8",
    "openai": "^6.22.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-intersection-observer": "^10.0.3",
    "sharp": "^0.34.5",
    "sort-es": "^1.7.18",
    "vercel": "^48.12.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@cloudflare/next-on-pages": "^1.13.16",
    "@tailwindcss/postcss": "^4.2.1",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "drizzle-kit": "^0.31.9",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
