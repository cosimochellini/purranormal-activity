{
  "name": "purranormal-activity",
  "version": "0.14.3",
  "private": true,
  "scripts": {
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "dev": "npm run increase && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "format": "biome format . --write",
    "lint:fix": "biome check --write .",
    "generate": "pnpm run increase && npx drizzle-kit push",
    "increase": "npm --no-git-tag-version version patch",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome format --write"
    ]
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.58",
    "@aws-sdk/client-s3": "3.726.1",
    "@libsql/client": "^0.15.15",
    "@tabler/icons-react": "^3.35.0",
    "ai": "^5.0.84",
    "classnames": "^2.5.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "next": "16.0.1",
    "next-view-transitions": "^0.3.4",
    "nuqs": "^2.7.2",
    "openai": "^6.7.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-intersection-observer": "^10.0.0",
    "sharp": "^0.34.4",
    "sort-es": "^1.7.17",
    "vercel": "^48.7.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@cloudflare/next-on-pages": "^1.13.16",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "drizzle-kit": "^0.31.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
